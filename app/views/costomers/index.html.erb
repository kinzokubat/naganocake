<div class="container">
<div class="row">
<div class="col-xs-12">
<h2>会員一覧</h2>
  <table class='table'>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th></th>
      <th>ステータス</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @costomers.each do |costomer| %>
      <tr>
        <td><%=link_to costomer.id, admin_costomers_show_path(costomer.id) %></td>
        <td><%= costomer.first_name %></td>
        <td><%= costomer.email %></td>
        <!--<td><= costomer.is_delete %></td>-->
      </tr>
     <% end %>
  </tbody>
</table>
</div>
</div>
</div>

